@model IEnumerable<JobPortal_ManagementSystem.Models.User>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GetDetails</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Password)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Phone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TenthPercentageOrGrade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TwelfthPercentageOrGrade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GraduationGradeOrPercentage)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PostGraduationGradeOrPercentage)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ResumePath)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Password)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TenthPercentageOrGrade)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TwelfthPercentageOrGrade)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GraduationGradeOrPercentage)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PostGraduationGradeOrPercentage)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ResumePath)
            </td>
            <td>
                @Html.ActionLink("Edit", "EditDetails", new { Id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
